	</parameter>
	<queryString>
		<![CDATA[SELECT DATE_FORMAT(inv_date,'%Y') as myYear,DATE_FORMAT(inv_date,'%m') as myMonthNum,
	DATE_FORMAT(inv_date,'%M') as myMonth,
	sum( invd_qty * invd_price) AS  value
FROM inv_invoices, inv_invoice_dtls
		WHERE inv_id=invd_inv_id
		AND inv_status="POSTED"
		 AND ((inv_date BETWEEN cast_date( $P{FROM_DATE} ) AND cast_date( $P{TO_DATE} )) OR($P{FROM_DATE} IS NULL))		
GROUP BY DATE_FORMAT(inv_date,'%Y'),DATE_FORMAT(inv_date,'%m')
order by myYear,myMonthNum]]>
	</queryString>
