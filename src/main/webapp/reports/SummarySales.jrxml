	<queryString>
		<![CDATA[SELECT YEAR(inv_date) as myYear,
SUM(CASE WHEN MONTH(inv_date)=1 THEN value ELSE 0 END) AS January,
 SUM(CASE WHEN MONTH(inv_date)=2 THEN value ELSE 0 END) AS February,
 SUM(CASE WHEN MONTH(inv_date)=3 THEN value ELSE 0 END) AS March,
 SUM(CASE WHEN MONTH(inv_date)=4 THEN value ELSE 0 END) AS April,
 SUM(CASE WHEN MONTH(inv_date)=5 THEN value ELSE 0 END) AS May,
 SUM(CASE WHEN MONTH(inv_date)=6 THEN value ELSE 0 END) AS June,
 SUM(CASE WHEN MONTH(inv_date)=7 THEN value ELSE 0 END) AS July,
 SUM(CASE WHEN MONTH(inv_date)=8 THEN value ELSE 0 END) AS August,
 SUM(CASE WHEN MONTH(inv_date)=9 THEN value ELSE 0 END) AS September,
 SUM(CASE WHEN MONTH(inv_date)=10 THEN value ELSE 0 END) AS October,
 SUM(CASE WHEN MONTH(inv_date)=11 THEN value ELSE 0 END) AS November,
 SUM(CASE WHEN MONTH(inv_date)=12 THEN value ELSE 0 END) AS December
 
 FROM
(SELECT inv_status,inv_date,(invd_qty * invd_price) AS value 
FROM inv_invoices, inv_invoice_dtls
WHERE inv_id=invd_inv_id
AND inv_status="POSTED"
AND ((inv_date BETWEEN cast_date( $P{FROM_DATE} ) AND cast_date( $P{TO_DATE} )) OR($P{FROM_DATE} IS NULL))
)A
GROUP BY myYear]]>
	</queryString>
